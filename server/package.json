{
  "name": "server",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8 <9"
  },
  "scripts": {
    "migrate:make": "npx knex migrate:make",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:run": "npx knex migrate:latest",
    "seed:run": "npx knex seed:run",
    "start": "nodemon src/server.js",
    "lint:js": "npx eslint --ext .js src",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@fastify/cors": "7.0.0",
    "@fastify/static": "5.0.2",
    "axios": "0.27.2",
    "bcrypt": "5.0.1",
    "dotenv": "16.0.1",
    "fastify": "3.29.4",
    "fastify-multer": "2.0.2",
    "fastify-plugin": "3.0.1",
    "form-data": "4.0.0",
    "jsonwebtoken": "8.5.1",
    "knex": "2.0.0",
    "nodemon": "2.0.16",
    "objection": "3.0.1",
    "pg": "8.7.3",
    "pg-hstore": "2.3.4",
    "qs": "6.10.3",
    "socket.io": "4.5.0",
    "shared": "file:../shared"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "15.0.0",
    "pino-pretty": "7.6.1"
  }
